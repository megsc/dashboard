<%= link_to 'Back', login_path %>


<h1>Coursecontents</h1>


<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Content name</th>
      <th>Content description</th>
      <th colspan="3"></th>
    </tr>
  </thead>


  <tbody>
    <% @coursecontents.each do |coursecontent| %>
      <tr>
        <td><%= coursecontent.course_id %></td>
        <td><%= coursecontent.content_name %></td>
        <td><%= coursecontent.content_description %></td>
        <td><%= link_to 'Edit', edit_coursecontent_path(coursecontent) %></td>
        <td><%= link_to 'Destroy', coursecontent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Add Course Content', new_coursecontent_path %>
